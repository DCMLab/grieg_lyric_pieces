        "datatype": "integer"
      },
      {
        "titles": "duration",
        "datatype": {
          "base": "string",
          "format": "-?\\d+(?:\\/\\d+)?"
        }
      },
      {
        "titles": "gracenote",
        "datatype": "string"
      },
      {
        "titles": "nominal_duration",
        "datatype": {
          "base": "string",
          "format": "-?\\d+(?:\\/\\d+)?"
        }
      },
      {
